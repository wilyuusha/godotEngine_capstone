[application]

config/name="Capstone Project"
run/main_scene="res://Scenes/BootSplash.tscn"
boot_splash/show_image=false
config/icon="res://icon.png"

[autoload]

SceneTransition="*res://Scenes/autoload/scene_transition.tscn"
AutoPositionsavedPosition="*res://Scenes/autoload/AutoPositionsaved_position.tscn"
PlayerInventory="*res://Scripts/autoload/playerInventory.gd"
JsonScript="*res://Scripts/json/jsonScript.gd"
SoundManager="*res://Scenes/autoload/SoundManager.tscn"
Global="*res://Scripts/autoload/Global.gd"
Global_LoadingScreen="*res://Scenes/autoload/load.gd"
SaveLoadState="*res://SaveLoad/SaveLoadState.gd"
